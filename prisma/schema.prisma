generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Project {
  id         String @id @default(cuid())
  p_name     String
  p_url      String @unique
  p_platform String

  @@map("projects")
}
